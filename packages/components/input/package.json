{
	"name": "@saftox-ui/input",
	"author": "Serhii Zaitsev <ceo@saftox.com>",
	"version": "0.0.1",
	"description": "A versatile input component for capturing user text input, featuring customizable styles and advanced functionality.",
	"keywords": ["input"],
	"private": true,
	"main": "./src/index.ts",
	"sideEffects": false,
	"files": ["dist"],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saftox-com/saftox-ui.git",
		"directory": "packages/components/input"
	},
	"bugs": {
		"url": "https://github.com/saftox-com/saftox-ui/issues"
	},
	"scripts": {
		"build": "tsup src --dts",
		"build:fast": "tsup src",
		"dev": "pnpm build:fast --watch",
		"dev:vite": "vite serve dev",
		"clean": "rimraf dist .turbo",
		"typecheck": "tsc --noEmit",
		"prepack": "clean-package",
		"postpack": "clean-package restore"
	},
	"peerDependencies": {
		"solid-js": ">=1.8.17"
	},
	"devDependencies": {
		"@saftox-ui/variants": "0.0.1"
	},
	"clean-package": "../../../configs/clean-package.config.json"
}
